{
  "version": "0.0.0-development",
  "license": "MIT",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "files": [
    "dist",
    "src"
  ],
  "engines": {
    "node": ">=18.0.0"
  },
  "scripts": {
    "start": "tsdx watch",
    "build": "tsdx build",
    "test": "tsdx test --passWithNoTests",
    "lint": "tsdx lint",
    "commit": "git-cz",
    "prepare": "tsdx build",
    "size": "size-limit",
    "analyze": "size-limit --why",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook",
    "semantic-release": "semantic-release --branches main"
  },
  "peerDependencies": {
    "react": ">=16"
  },
  "husky": {
    "hooks": {
      "pre-commit": "tsdx lint"
    }
  },
  "prettier": {
    "printWidth": 80,
    "semi": true,
    "singleQuote": true,
    "trailingComma": "es5"
  },
  "name": "second-semantic-release-test",
  "author": "Sandi Sopian",
  "module": "dist/second-semantic-release-test.esm.js",
  "size-limit": [
    {
      "path": "dist/second-semantic-release-test.cjs.production.min.js",
      "limit": "10 KB"
    },
    {
      "path": "dist/second-semantic-release-test.esm.js",
      "limit": "10 KB"
    }
  ],
  "devDependencies": {
    "@babel/core": "^7.20.12",
    "@size-limit/preset-small-lib": "^8.1.2",
    "@storybook/addon-essentials": "^6.5.16",
    "@storybook/addon-info": "^5.3.21",
    "@storybook/addon-links": "^6.5.16",
    "@storybook/addons": "^6.5.16",
    "@storybook/react": "^6.5.16",
    "@types/react": "^18.0.28",
    "@types/react-dom": "^18.0.10",
    "babel-loader": "^9.1.2",
    "cz-conventional-changelog": "^3.3.0",
    "husky": "^8.0.3",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-is": "^18.2.0",
    "semantic-release": "^20.1.0",
    "size-limit": "^8.1.2",
    "tsdx": "^0.14.1",
    "tslib": "^2.5.0",
    "typescript": "^4.9.5"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/SandiSopian/Automate-semantic-release-second-projects.git"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
